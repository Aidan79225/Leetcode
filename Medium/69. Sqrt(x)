class Solution {
public:
    int mySqrt(int x) {
        int r=x,l=0,mid,temp;
        if(x==1)return 1;
        if(r>46340)r=46341;
        while(r>l){
            mid = (r+l)/2;
            temp =mid*mid;
            if(temp>x){
                r =mid;
            }else if(temp<x){
                l=mid;
            }else
                return mid;
            if(r==l+1)return l;
        }
        return r; 
    }
};
